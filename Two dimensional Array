#include<stdio.h>
void main()
{
 int a[3][3],i,j;
 printf("Enter array elements:");
 for(i=0;i<3;i++)
 {
  for(j=0;j<3;j++)
  {
   printf("\n Enter a[%d][%d]=",i,j);
   scanf("%d",&a[i][j]);
  }
 }
  printf("Array elements are:");
  for(i=0;i<3;i++)
  {
   for(j=0;j<3;j++)
   {
    printf("\t a[%d][%d]=%d",i,j,a[i][j]);
   }
  }
}
